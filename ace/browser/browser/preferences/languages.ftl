languages-window =
    .title = Basa
    .style = luwah: 30em
languages-close-key =
    .key = w
languages-description = Ônwèb nyoë meujan-jan dipeutaba leubèh dari saboh basa. Piléh basa untôk peuleumah nibak ônwèb nyoë, nyang meu-urôt seusuwai deungon préfèren
languages-customize-spoof-english =
    .label = Meulakèë vèrsi Inggréh nibak ônwèb untôk meupeu-èk privasi
languages-customize-moveup =
    .label = Peu-ék
    .accesskey = P
languages-customize-movedown =
    .label = Peutrôn
    .accesskey = P
languages-customize-remove =
    .label = Sampôh
    .accesskey = S
languages-customize-select-language =
    .placeholder = Piléh keu tamah basa...
languages-customize-add =
    .label = Tamah
    .accesskey = T
# The pattern used to generate strings presented to the user in the
# locale selection list.
#
# Example:
#   Icelandic [is]
#   Spanish (Chile) [es-CL]
#
# Variables:
#   $locale (String) - A name of the locale (for example: "Icelandic", "Spanish (Chile)")
#   $code (String) - Locale code of the locale (for example: "is", "es-CL")
languages-code-format =
    .label = { $locale } [{ $code }]
