profile-selection-window =
    .title = { -brand-short-name } - ব্যবহারকারীর প্রোফাইল নির্বাচন করুন
profile-selection-button-accept =
    .label = { -brand-short-name } আরম্ভ করুন
profile-selection-button-cancel =
    .label = প্রস্থান
profile-selection-new-button =
    .label = প্রোফাইল নির্মাণ...
    .accesskey = C
profile-selection-rename-button =
    .label = প্রোফাইলের নাম পরিবর্তন...
    .accesskey = R
profile-selection-delete-button =
    .label = প্রোফাইল অপসারণ...
    .accesskey = D

## Messages used in the profile manager

profile-manager-description = আপনার ব্যবহৃত বৈশিষ্ট্যাবলী, পছন্দসমূহ ও অন্যান্য বিষয়বস্তু সম্পর্কে { -brand-short-name }-র দ্বারা আপনার প্রোফাইলের মধ্যে তথ্য সংরক্ষণ করা হয়।
profile-manager-work-offline =
    .label = অফলাইন অবস্থায় কাজ করুন
    .accesskey = অ
profile-manager-use-selected =
    .label = শুরুর সময় জিজ্ঞেস না করেই এই প্রোফাইলটি ব্যবহার করা শুরু করে দেওয়া হোক
    .accesskey = s
