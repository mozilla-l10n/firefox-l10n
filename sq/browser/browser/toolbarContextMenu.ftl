toolbar-context-menu-bookmarks-show-other-bookmarks =
    .label = Shfaq Faqerojtës të Tjerë
    .accesskey = T
toolbar-context-menu-menu-bar-cmd =
    .toolbarname = Shtyllë Menush
    .accesskey = M
