webpage-languages-window =
    .title = Mpustazziuna lingua dâ pàggina
    .style = width: 40em
languages-close-key =
    .key = w
languages-description = Li pàggini certi voti vennu ufferti nna cchiù assai di na lingua. Scegghi li lingui pi mustrari sti pàggini, 'n ùrdini di prifirenza
languages-customize-spoof-english =
    .label = P'aviri cchiù privatizza, spia la virsiuni ngrisi di sti pàggini
languages-customize-moveup =
    .label = Sposta supra
    .accesskey = U
languages-customize-movedown =
    .label = Sposta sutta
    .accesskey = D
languages-customize-remove =
    .label = Leva
    .accesskey = R
languages-customize-select-language =
    .placeholder = Scegghi na lingua di agghiùnciri…
languages-customize-add =
    .label = Agghiunci
    .accesskey = A
# The pattern used to generate strings presented to the user in the
# locale selection list.
#
# Example:
#   Icelandic [is]
#   Spanish (Chile) [es-CL]
#
# Variables:
#   $locale (String) - A name of the locale (for example: "Icelandic", "Spanish (Chile)")
#   $code (String) - Locale code of the locale (for example: "is", "es-CL")
languages-code-format =
    .label = { $locale } [{ $code }]
languages-active-code-format =
    .value = { languages-code-format.label }
browser-languages-window =
    .title = Mpustazziuna lingua { -brand-short-name }
    .style = width:40em
browser-languages-description = { -brand-short-name } mustrarà la prima lingua comu pridifinuta e si serbi mustrarà autri lingui nna l'ùrdini ca cumparìscinu.
browser-languages-search = Cerca autri lingui...
browser-languages-searching =
    .label = Staju circannu li lingui...
browser-languages-downloading =
    .label = Staju scarricannu...
browser-languages-select-language =
    .label = Scegghi na lingua d'agghiùnciri...
    .placeholder = Scegghi na lingua d'agghiùnciri...
browser-languages-installed-label = Lingui nstallati
browser-languages-available-label = Lingui dispunìbbili
browser-languages-error = { -brand-short-name } a stu mumentu nun po aggiurnari li to lingui. Cuntrolla si sì cunnessu a internet o riprova.
