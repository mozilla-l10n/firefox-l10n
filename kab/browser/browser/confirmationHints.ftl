# Variables:
#   $tabCount (Number): The number of duplicate tabs closed, at least 1.
confirmation-hint-duplicate-tabs-closed =
    { $tabCount ->
        [one] Yettwamdel { $tabCount } yiccer
       *[other] Ttwamedlen { $tabCount } waccaren
    }
