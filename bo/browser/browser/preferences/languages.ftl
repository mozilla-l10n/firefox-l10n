webpage-languages-window =
    .title = དྲ་ངོས་ཀྱི་སྐད་ཡིག་སྒྲིག་འགོད
    .style = width: 40em
languages-close-key =
    .key = w
languages-description = Web pages are sometimes offered in more than one language. Choose languages for displaying these web pages, in order of preference
languages-customize-spoof-english =
    .label = Request English versions of web pages for enhanced privacy
languages-customize-moveup =
    .label = གོང་དུ་སྤོར
    .accesskey = U
languages-customize-movedown =
    .label = འོག་ཏུ་སྤོར
    .accesskey = D
languages-customize-remove =
    .label = བསུབ
    .accesskey = R
languages-customize-select-language =
    .placeholder = ཁ་སྣོན་བྱེད་དགོས་པའི་སྐད་ཡིག་ཞིག་གདམ་་་
languages-customize-add =
    .label = ཁ་སྣོན
    .accesskey = A
# The pattern used to generate strings presented to the user in the
# locale selection list.
#
# Example:
#   Icelandic [is]
#   Spanish (Chile) [es-CL]
#
# Variables:
#   $locale (String) - A name of the locale (for example: "Icelandic", "Spanish (Chile)")
#   $code (String) - Locale code of the locale (for example: "is", "es-CL")
languages-code-format =
    .label = { $locale }  [{ $code }]
languages-active-code-format =
    .value = { languages-code-format.label }
browser-languages-window =
    .title = { -brand-short-name }་ཡི་སྐད་ཡིག་སྒྲིག་འགོད
    .style = width: 40em
browser-languages-description = { -brand-short-name } will display the first language as your default and will display alternate languages if necessary in the order they appear.
browser-languages-search = སྐད་རིགས་གཞན་འཚོལ་བ་་་
browser-languages-searching =
    .label = སྐད་ཡིག་འཚོལ་བཞིན་ཡོད་་་
browser-languages-downloading =
    .label = མར་འཇུག་བཞིན་ཡོད་་་
browser-languages-select-language =
    .label = ཁ་སྣོན་བྱེད་དགོས་པའི་སྐད་ཡིག་ཞིག་གདམ་་་
    .placeholder = ཁ་སྣོན་བྱེད་དགོས་པའི་སྐད་ཡིག་ཞིག་གདམ་་་
browser-languages-installed-label = བསྒྲིག་ཡོད་པའི་སྐད་ཡིག
browser-languages-available-label = སྤྱོད་རུང་བའི་སྐད་ཡིག
browser-languages-error = { -brand-short-name } can’t update your languages right now. Check that you are connected to the internet or try again.
